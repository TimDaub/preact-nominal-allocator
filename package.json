  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/TimDaub/preact-nominal-allocator/issues"
  },
  "homepage": "https://github.com/TimDaub/preact-nominal-allocator#readme",
  "dependencies": {
    "htm": "3.1.0",
    "preact": "10.6.4",
    "jss": "10.9.0",
    "jss-preset-default": "10.9.0"
  },
  "devDependencies": {
    "concurrently": "6.5.1",
    "esbuild": "0.14.10"
  }
}
