{
  "name": "preact-nominal-allocator",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild ./src/index.js --bundle --format=esm --outfile=www/js/out.js",
    "watch": "esbuild ./src/index.js --bundle --format=esm --outfile=www/js/out.js --watch",
    "serve": "esbuild --serve --servedir=www",
    "dev": "concurrently \"npm run serve\" \"npm run watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimDaub/preact-nominal-allocator.git"
  },
